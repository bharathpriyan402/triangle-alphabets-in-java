import java.util.*;
public class Main{
public static void main (String args[]){
Scanner scan=new Scanner(System.in);
int i,j;
int letter=65;
System.out.print("Enter the number of rows");
int rows=scan.nextInt();
for(i=0;i<=rows;i++){
  for(j=0;j<=i;j++){
  System.out.print((char)(j+letter));
}
System.out.println();
}
}
}
